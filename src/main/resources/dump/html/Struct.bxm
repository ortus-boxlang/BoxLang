<bx:script>
	expandRoot = expand || expand eq null
</bx:script>
<bx:output>
	<div class="bx-dump">
		<table class="bx-table-primary" title="#posInCode#">
			<!--- TODO: Special handling of CGI scope to show all keys --->

			<!--- SCOPE HEADERS --->
			<bx:if var instanceof "ortus.boxlang.runtime.scopes.IScope" >
				<bx:if var.len() || var instanceof 'CGIScope' >
					<caption
						role="button"
						tabindex="0"
						<bx:if expandRoot>open</bx:if>
						onclick="this.toggleAttribute( 'open' ); this.nextElementSibling.classList.toggle( 'd-none' )"
						onkeyup="if( event.key === 'Enter' ){ this.toggleAttribute( 'open' ); this.nextElementSibling.classList.toggle( 'd-none' );}"
					>
						<strong>
							<bx:if label.len() >#label# - </bx:if>
							#encodeForHTML( var.getName().getName().toUpperCase() )# Scope:
							<bx:if var instanceof 'CGIScope'>
								<bx:if top gt 0 && top lt var.getDumpKeys().size()>#top#/</bx:if>#var.getDumpKeys().size()# items
							<bx:else>
								<bx:if top gt 0 && top lt var.len()>#top#/</bx:if>#var.len()# items
							</bx:if>
						</strong>
					</caption>
				<bx:else>
					<caption>
						<strong>
							<bx:if label.len() >#label# - </bx:if>
							#encodeForHTML( var.getName().getName().toUpperCase() )# Scope:
							<bx:if top gt 0 && top lt var.len()>#top#/</bx:if>#var.len()# items
						</strong>
					</caption>
				</bx:if>

			<!--- STRUCTHEADERS --->
			<bx:else>
				<bx:if var.len() >
					<caption
						role="button"
						tabindex="0"
						<bx:if expandRoot>open</bx:if>
						onclick="this.toggleAttribute('open');this.nextElementSibling.classList.toggle('d-none')"
						onkeyup="if(event.key === 'Enter'){ this.toggleAttribute('open');this.nextElementSibling.classList.toggle('d-none');}"
					>
						<strong>
							<bx:if label.len() >#label# - </bx:if>
							Struct:
							<bx:if top gt 0 && top lt var.len()>#top#/</bx:if>#var.len()# items
						</strong>
					</caption>
				<bx:else>
					<caption>
						<strong>
							<bx:if label.len() >#label# - </bx:if>
							Struct:
							<bx:if top gt 0 && top lt var.len()>#top#/</bx:if>#var.len()# items
						</strong>
					</caption>
				</bx:if>
			</bx:if>

			<!--- STRUCT BODY --->
			<tbody
				<bx:if !expandRoot>class="d-none"</bx:if>
			>
				<bx:script>
					theCollection = var;
					if( var instanceof 'CGIScope' ) {
						theCollection = var.getDumpKeys();
					}
					index = 1;
					for ( key in theCollection ) {
						if( top > 0 && index++ > top ) {
							break;
						}
						```
						<bx:if NOT isCustomFunction( var[ key ] ) >
							<tr>
								<th
									scope="row"
									class="bx-dhSt"
									valign="top"
									onclick="this.nextElementSibling.style.display = this.nextElementSibling.style.display === 'none' ? 'block' : 'none'"
									>
									#encodeForHTML( key )#
								</th>
								<td>
									<bx:set dump( var : var[ key ], top : top, expand : expand ) >
								</td>
							</tr>
						</bx:if>
						```
					}
				</bx:script>
			</tbody>
		</table>
	</div>
</bx:output>
