<bx:output>
<div class="bx-dump" title="#encodeForHTML( posInCode )#">
	<span class="bx-dwSv">
		<span class="bx-dhSv">
			<bx:if label.len() >#label# - </bx:if>
			Instant:
		</span>
		<span>#encodeForHTML( var.toString() )#</span>
		<br>
		<span class="bx-dhSv">
			Full:
		</span>
		<span>#encodeForHTML( dateTimeFormat( var, "full" ) )#</span>
	</span>
</div>
</bx:output>
