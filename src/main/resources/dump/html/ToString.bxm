<!--- ToString template --->
<bx:output>
<div class="bx-dump" title="#encodeForHTML( posInCode )#">
	<span class="bx-dwSv">
		<span class="bx-dhSv">
			<bx:if label.len() >
				#encodeForHTML( label )#
			<bx:else>
				#var.getClass().getCanonicalName()#
			</bx:if>
		</span>
		<span>
			#encodeForHTML( var.toString() )#
		</span>
	</span>
</div>
</bx:output>
